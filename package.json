{
  "name": "@jobello/jobber-ui",
  "version": "1.34.0",
  "description": "Jobello UI library",
  "main": "dist/jobber-ui.cjs.js",
  "module": "dist/jobber-ui.esm.js",
  "browser": "dist/jobber-ui.umd.js",
  "repository": "git@github.com:Jobber-project/jobber-ui.git",
  "author": "Jesper Johansson <johansson.jesper@outlook.com>",
  "homepage": "https://jobber-project.github.io/jobber-ui",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "storybook": "start-storybook -p 6006 -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.1",
    "styled-reset": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/npm": "^8.0.1",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@storybook/addon-a11y": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/react": "^6.4.8",
    "@storybook/storybook-deployer": "^2.8.10",
    "@svgr/rollup": "^6.1.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "rollup": "^2.60.1",
    "rollup-plugin-ts": "^2.0.4",
    "semantic-release": "^18.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": "next",
    "rules": {
      "import/order": [
        "error",
        {
          "newlines-between": "always"
        }
      ]
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
